{
  "name": "drpshtiwan/livewire-async-select",
  "description": "A powerful async select component for Laravel Livewire with Alpine.js - a modern alternative to Select2",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "livewire",
    "alpine",
    "alpinejs",
    "select",
    "select2",
    "async-select",
    "dropdown",
    "autocomplete",
    "ajax-select"
  ],
  "authors": [
    {
      "name": "Dr. Pshtiwan Mahmood",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "livewire/livewire": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "DrPshtiwan\\LivewireAsyncSelect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DrPshtiwan\\LivewireAsyncSelect\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.34",
    "pestphp/pest-plugin-laravel": "^2.4",
    "laravel/pint": "^1.20.0|^1.25.0",
    "guzzlehttp/guzzle": "^7.5"
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "pest": "vendor/bin/pest",
    "pint": "vendor/bin/pint",
    "lint": "vendor/bin/pint"
  },
  "extra": {
    "laravel": {
      "providers": [
        "DrPshtiwan\\LivewireAsyncSelect\\AsyncSelectServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "homepage": "https://livewire-select.thejano.com",
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
